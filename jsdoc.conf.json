{ 
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"] 
    },  
    "source": {
        "include": [ "package.json", "src", "test" ]
    },   
    "plugins": [ 
		"plugins/markdown"   
	], 
    "templates": {    
        "cleverLinks": false,
        "monospaceLinks": false,
		"logoFile": "",   
		"monospaceLinks": false,     
		"dateFormat": "ddd MMM Do YYYY",  
		"outputSourceFiles": false,
		"outputSourcePath": false,
		"systemName": "dibello.js", 
		"footer": "&nbsp;&nbsp;&nbsp;Thanks for reading this far!",
		"copyright": "Copyright Â© 2015 The contributors to the Skate project",
		"navType": "inline",
		"theme": "yeti",
		"linenums": true,
		"collapseSymbols": true, 
		"inverseNav": true,  
		"highlightTutorialCode": true,
		"protocol": "html://",
		"methodHeadingReturns": false
    },
	"opts": {
		"recurse": true,
		"readme": "README.md",
		"template": "doc/template/template"
	},
	"markdown": {
		"excludeTags": ["see"]
	}
}
